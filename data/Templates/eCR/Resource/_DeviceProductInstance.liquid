{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Device",
        "id":"{{ID}}",
        "identifier":
  		[
            {% assign ids = deviceEntry.id | to_array -%}
            {% for i in ids -%}
                {% include 'DataType/Identifier' Identifier: i -%}
            {% endfor -%}
        ],
		"type": { {% include 'DataType/CodeableConcept' CodeableConcept: deviceEntry.playingDevice.code -%} },
    },
    "request":{
        "method":"PUT",
        "url":"Device/{{ID}}",
    },
},
