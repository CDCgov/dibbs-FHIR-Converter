{
    "fullUrl":"urn:uuid:{{ ID }}",
    "resource":{
        "resourceType": "Observation",
        "id":"{{ ID }}",
        "meta":
        {
            "profile":
            [
                "http://hl7.org/fhir/us/ecr/StructureDefinition/us-ph-transportation-details",
            ],
        },
        "category" : [
            {
                "coding" :[
                    {
                        "system" : "http://terminology.hl7.org/CodeSystem/v3-ActClass",
                        "code" : "TRNS",
                        "display" : "Transportation",
                    }
                ],
            }
        ],
        "code": {
            "coding": [
                {
                    "system": "http://snomed.info/sct",
                    "code": "424483007",
                    "display": "Transportation details",
                }
            ]
        },
        "status":
             {% if transitOrganizer.statusCode._ -%}
                "{{ transitOrganizer.statusCode._}}",
            {% else -%}
                "{{ transitOrganizer.statusCode.code | get_property: 'ValueSet/ObservationStatus' }}",
            {% endif -%}
        {% include 'Utils/ValueHelper' value: transitOrganizer.code %}
        {% include 'Utils/EffectiveTime' effectiveTime: transitOrganizer.effectiveTime %}
        "component":
        [
            {% assign comps = transitOrganizer.component | to_array %}
            {% for comp in comps %}
                {% if comp.observation.value %}
                    {
                        "code" : { {% include 'DataType/CodeableConcept' CodeableConcept: comp.observation.code %}},
                        {% include 'Utils/ValueHelper' value: comp.observation.value %}
                    },
                {% endif %}
            {% endfor %}
        ],
    },
    "request":{
        "method":"PUT",
        "url":"Observation/{{ ID }}",
    },
},
