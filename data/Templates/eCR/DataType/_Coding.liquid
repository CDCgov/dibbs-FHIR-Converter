"code": "{{- Coding.code | strip -}}",
{%- capture codeSystemName -%}
  {%- include 'ValueSet/SystemReference' code: Coding.codeSystem -%}
{%- endcapture -%}
{%- assign display = nil - %}
"system": "{{ codeSystemName | prepend_id -}}",
{%- if codeSystemName contains "loinc" -%}
  {%- assign display = Coding.code | get_loinc_name -%}
{%- endif -%}
{%- if codeSystemName contains "rxnorm" and display == nil -%}
  {%- assign display = Coding.code | get_rxnorm_name -%}
{%- endif -%}
{%- if Coding.displayName and display == nil -%}
  {%- assign display = Coding.displayName | clean_string_from_tabs | strip -%}
{%- endif -%}
"display": "{{display}}",
