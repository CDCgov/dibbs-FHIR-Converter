{% evaluate immunizationId using 'Utils/GenerateId' obj: entry.substanceAdministration -%}
{% assign entrySubs = entry.substanceAdministration | to_array -%}
{% for entrySub in entrySubs -%}
    {{ entrySub.consumable | to_array | batch_render: 'Entry/Immunization/entry_consumable', 'subConsumable' }}
{% endfor -%}
{%- assign entryObs = entry.observation | to_array -%}
{%- for entryOb in entryObs -%}
    {%- if entryOb.templateId.root == '2.16.840.1.113883.10.20.15.2.3.55' -%}
        {% assign obsId = entryOb | to_json_string | generate_uuid -%}
        {%- include 'Resource/ObservationVaccineCredentialPatientAssertion' observationEntry: entryOb, ID: obsId -%}
    {%- endif -%}
{%- endfor -%}