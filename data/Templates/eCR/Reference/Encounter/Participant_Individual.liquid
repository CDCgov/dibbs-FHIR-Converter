{
    "resource":{
        "resourceType": "Encounter",
        "id":"{{ ID }}",
        "participant":
        [
            {
                {% if type != nil -%}
                    "type": [
                        {
                            "coding": [
                                {
                                    "system": "http://terminology.hl7.org/CodeSystem/v3-ParticipationType",
                                    "code": "{{ type }}"
                                },

                            ],
                        },
                    ],
                {% endif -%}
                {% if time != nil -%}
                    "period":
                    {
                        {% include 'DataType/Period', Period: time -%}
                    },
                {% endif -%}
                "individual":
                {
                    "reference":"{{ REF }}",
                },
            },
        ],
    },
},
