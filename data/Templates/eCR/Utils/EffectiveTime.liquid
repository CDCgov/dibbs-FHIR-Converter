{% assign _timeType = timeType | default: "effective" %}
{% assign _effectiveTime = effectiveTime %}
{% if _effectiveTime.width.value %}
    {% assign _effectiveTime = effectiveTime | format_width_as_period %}
{% endif %}
{% if _effectiveTime.low.value or _effectiveTime.high.value -%}
    "{{ _timeType }}Period": { {% include 'DataType/Period', Period: _effectiveTime %} },
{%- elsif _effectiveTime.value -%}
    "{{ _timeType }}DateTime":"{{ _effectiveTime.value | format_as_date_time }}",
{%- elsif _effectiveTime.center.value -%}
    "{{ _timeType }}DateTime":"{{ _effectiveTime.center.value | format_as_date_time }}",
{%- endif -%}
