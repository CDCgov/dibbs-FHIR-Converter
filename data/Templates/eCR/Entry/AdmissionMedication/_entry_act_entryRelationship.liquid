{% assign fullManufacturedMaterialId = manufacturedMaterialId | prepend: 'Medication/' -%}
{% include 'Reference/MedicationAdministration/MedicationReference' ID: substanceAdministrationId, REF: fullManufacturedMaterialId -%}



